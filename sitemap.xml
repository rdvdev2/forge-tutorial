---
layout: null
exclude: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- for web_page in site.pages -%}
        {%- unless web_page.exclude == true or web_page.url == "assets/css/style.css"-%}
            <url>
                <loc>{{ site.url }}{{ web_page.url | remove: "index.html" }}</loc>
                <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
                <changefreq>monthly</changefreq>
                <priority>0.5</priority>
            </url>
        {%- endunless -%}
    {%- endfor -%}
</urlset>